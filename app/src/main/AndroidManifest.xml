<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tipsy">

    <application
        android:allowBackup="true"
        android:icon="@drawable/homebtn"
        android:label="Tipsy here!"
        android:logo="@drawable/rbtn"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tipsy">
        <activity android:name=".friendset"
            android:label="@string/title_activity_friendset"
            android:theme="@style/Theme.Tipsy.NoActionBar" >

        </activity>
        <activity
            android:name=".account"
            android:label="@string/title_activity_account"
            android:theme="@style/Theme.Tipsy.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>